{
  "name": "code-workshop-kit-extension",
  "displayName": "code-workshop-kit",
  "description": "code-workshop-kit extension. The future of remote code workshops!",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/code-workshop-kit/vscode-extension.git"
  },
  "publisher": "code-workshop-kit",
  "icon": "cwk.png",
  "activationEvents": [
    "workspaceContains:**/cwk.config.js"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "release": "vsce publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.14.1",
    "@types/esm": "^3.2.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "glob": "^7.1.6",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "vsce": "^1.87.1",
    "vscode-test": "^1.5.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@babel/traverse": "^7.13.13",
    "@babel/types": "^7.13.14",
    "@types/babel__core": "^7.1.14",
    "code-workshop-kit": "^2.0.3",
    "esm": "^3.2.25",
    "vsls": "^1.0.3015"
  }
}
